+		timer {
+			clock-frequency = <240000>;
+		};
+
+		smmu500: arm,smmu@1E00000 {
+			status = "ok";
+		};
+
+		glink_adsp: qcom,glink-smem-native-xprt-adsp@4AA00000 {

+	base-addr = <0x50200000>;
+	m3-dump-addr = <0x52500000>;
+	etr-addr = <0x52600000>;
+	caldb-addr = <0x52700000>;
+	hremote-size = <0x2300000>;
+	tgt-mem-mode = <0x0>;
+#endif
+	board_id = <0xa4>;
+	caldb-size = <0x800000>;
+	status = "ok";
+};
+#endif
